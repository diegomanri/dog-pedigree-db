<p id="notice"><%= notice %></p>

<h1>Listing Owners</h1>

<table>
  <thead>
    <tr>
      <th>Ofname</th>
      <th>Omname</th>
      <th>Olname</th>
      <th>Opnumber</th>
      <th>Oemail</th>
      <th>Ocity</th>
      <th>Ozip</th>
      <th>Ostate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.ofname %></td>
        <td><%= owner.omname %></td>
        <td><%= owner.olname %></td>
        <td><%= owner.opnumber %></td>
        <td><%= owner.oemail %></td>
        <td><%= owner.ocity %></td>
        <td><%= owner.ozip %></td>
        <td><%= owner.ostate %></td>
        <td><%= link_to 'Show', owner %></td>
        <td><%= link_to 'Edit', edit_owner_path(owner) %></td>
        <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner', new_owner_path %>
