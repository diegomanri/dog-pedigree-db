<h2>Registered Dogs</h2>

<table class="table table-hover table-striped">
  <thead>
  <tr>
    <th>Dog name</th>
    <th>Breed</th>
    <th>Variety</th>
    <th>DOB</th>
    <th>Owner's Username</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @dogs.each do |dog| %>
      <tr>
        <td><%= dog.dname %></td>
        <td><%= dog.breed %></td>
        <td><%= dog.variety %></td>
        <td><%= dog.dob %></td>
        <td><%= link_to dog.user.username, dog.user %></td>
        <td><%= link_to 'View Dog', dog %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate @dogs %>